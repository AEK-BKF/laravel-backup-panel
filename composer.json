{
    "name": "BKF-DEV/laravel-backup-panel",
    "description": "An interface for Spatie Laravel Backup package",
    "homepage": "https://github.com/BKF-DEV/laravel-backup-panel",
    "keywords": [
        "backup",
        "database",
        "laravel-backup",
        "spatie"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "BKF-DEV",
            "email": "akaderb@gmail.com"
        }
    ],
    "require": {
        "spatie/laravel-backup": "^5.5"
    },
    "require-dev": {
        "orchestra/testbench": "^4.4",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "PavelMironchik\\LaravelBackupPanel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PavelMironchik\\LaravelBackupPanel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PavelMironchik\\LaravelBackupPanel\\LaravelBackupPanelServiceProvider"
            ]
        }
    }
}
